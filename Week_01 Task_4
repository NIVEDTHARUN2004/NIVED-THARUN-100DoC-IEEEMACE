#To find the sum of the array
def add(Arr):
    _sum=0
    for i in Arr:
        _sum += i
    return _sum
        
#To take an array as input
print('Enter The Integers\nseparate each element by space')
n=input()
n=n.split()
for i in range(len(n)):
    n[i]=int(n[i])
print("THE INPUT ARRAY IS",n)

sum_ = add(n)
print('The sum of the input=', sum_)


    

    
